ia poco visible ;"><biological_entity id="T82L2S1-182541" name="ramitas" type="structure"><character  name="shape" value="anguladas" constraint_preposition="hacia el ápice"></character ><character  name="pubescence" value="glabras" constraint_conjunction="o" constraint_preposition="con pubescencia poco visible ;"></character ></biological_entity><biological_entity id="T82L2S1-182542" name="ápice" type="structure"></biological_entity><biological_entity id="T82L2S2-182543" name="pubescencia" type="structure"></biological_entity></statement><statement id="T82L3" text=" estípulas ausentes ."><biological_entity id="T82L3S1-182544" name="estípulas" type="structure"><character  name="quantity" value="ausentes"></character ></biological_entity></statement><statement id="T82L4" text=" hojas simples , alternas , 4-14 × 1,5-6 cm , elíptico-oblongas u obovadas , ápice obtuso a redondeado , base decurrente sobre el pecíolo , glabras , con 5-7 venas secundarias , margen entero , a veces revoluto hacia la base ."><biological_entity id="T82L4S1-182545" name="hojas" constraint="-" constraint_preposition="con 5-7 venas secundarias - a veces revoluto hacia la base ." type="structure"><character  name="architecture" value="simples"></character ><character  name="arrangement" value="alternas"></character ><character  name="length" value="4-14" char_type="range_value" from="4" from_unit="cm" to="14" to_unit="cm"></character ><character  name="width" value="1,5-6" char_type="range_value" from="1,5" from_unit="cm" to="6" to_unit="cm"></character ><character  name="shape" value="elíptico-oblongas" constraint_conjunction="u"></character ><character  name="shape" value="obovadas"></character ><character  name="pubescence" value="glabras"></character ></biological_entity><biological_entity id="T82L4S5-182546" name="ápice" type="structure"><character  name="shape" value="obtuso" other_constraint="a" constraint_preposition="a redondeado"></character ></biological_entity><biological_entity id="T8